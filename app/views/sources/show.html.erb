<p id="notice"><%= notice %></p>

<p>
  <strong>Algorithm value:</strong>
  <%= @source.algorithm_value %>
</p>

<p>
  <strong>Label:</strong>
  <%= @source.label %>
</p>

<p>
  <strong>Language:</strong>
  <%= @source.language %>
</p>

<p>
  <strong>Selected:</strong>
  <%= @source.selected %>
</p>

<p>
  <strong>Selected by:</strong>
  <%= @source.selected_by %>
</p>

<p>
  <strong>Next step:</strong>
  <%= @source.next_step %>
</p>

<p>
  <strong>Render js:</strong>
  <%= @source.render_js %>
</p>

<p>
  <strong>Property:</strong>
  <%= @source.property.rdfs_class.name %> <%= @source.property.label %>
</p>

<p>
  <strong>Website:</strong>
  <%= @source.website.seedurl %>
</p>

<% if !@source.statements.first.nil? %>
  <%= link_to 'Test scrape', refresh_statement_path(@source.statements.first), method: :patch %> |
<% else %>
  Test scrape (to test a new source first refresh a webpage) |
<% end %>
<%= link_to 'Edit', edit_source_path(@source) %> |
<%= link_to 'Back', sources_path %>
