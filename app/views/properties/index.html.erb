

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Rdfs class</th>
      <th>Label</th>
      <th>Value datatype</th>
        <th>Expected Class</th>
      <th>Uri</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.id %></td>
        <td><%= property.rdfs_class.name if property.rdfs_class %></td>
        <td><%= property.label %></td>
        <td><%= property.value_datatype %></td>
        <td><%= property.expected_class %></td>
        <td><%= property.uri %></td>

        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
