<p id="notice"><%= notice %></p>


<h1>  Places for <%= params[:seedurl] %> <%= @places.count %> Event Series</h1>


<table>
<% @places.each do |place| %>
   <% place[1].each do |p| %>
   <tr>
   <td>
        <%=link_to place[0], statements_path(rdf_uri: place[0] ) %>
    </td>  
    <% if p.class == Array %>
        <td>
            <%= p[0] %>
        </td>
        <td>
            <%= p[1] %>
        </td>
        <td>
            <%= p[2] %>
        </td>
        <td>
            <%= p[3] %>
        </td>
        <td>
            <%= p[4] %>
        </td>
         <td>
            <%= p[5] %>
        </td>
         <td>
            <%= p[6] %>
        </td>
    <% else %> 
        <td>
            <%= p %>
        </td>
    <% end %>
    </tr>
    <% end %>
    
<% end %>
</table>