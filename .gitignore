# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
dump.rdp

# Heroku database dump with commands:
# heroku pg:backups:capture
# heroku pg:backups:download
# pg_restore --verbose --clean --no-acl --no-owner -h localhost -U saumier -d footlight-condenser_development latest.dump

.DS_Store
test/.DS_Store

# Ignore master key for decrypting credentials and more.
/config/master.key

# AWS
.aws_keys

# Yarn Berry (v2+)
.pnp.*
.yarn/cache/
.yarn/unplugged/
.yarn/build-state.yml
.yarn/install-state.gz

# Ignore NPM lockfile if using Yarn (v1+)
package-lock.json

# JS/CSS Build artifacts (adjust to your stack)
app/assets/builds/
public/assets/
app/assets/manifest*.json

# Coverage reports
coverage/

# Local dev scripts/logs
restore.log

# Backup/old fixtures
test/fixtures/*.yml.old

# IDE/editor files
.idea/
.vscode/
*~
